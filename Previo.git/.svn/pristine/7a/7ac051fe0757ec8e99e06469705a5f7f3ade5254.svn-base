<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frm_lyt_nomRevision"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondo"> 

    <ScrollView
        android:id="@+id/scr_nomRevision"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       android:background="@color/fondo"
        android:scrollbars="vertical" >
         <LinearLayout
            android:id="@+id/lyt_armar_nom_padre"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/lyt_armar_nom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:orientation="vertical" >
            
            <LinearLayout
                android:id="@+id/Layouth"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="fill_horizontal"
				style="@style/TitlePlayStore"
                android:orientation="horizontal" >
                
             <TextView
                android:id="@+id/txv_revisionNomTitulo"
               style="@style/TitlePlayStore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.06"
                android:text="@string/textViewRevisionNOM"
                android:textAppearance="?android:attr/textAppearanceLarge" 
                android:textStyle="bold"/>
             <ImageButton
                    android:id="@+id/imgb_revisionDeleteNom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"                                   
                    android:layout_weight=".25"
                    style="@style/TitlePlayStore"
                    android:src="@android:drawable/ic_menu_delete" />
            </LinearLayout>
       </LinearLayout>
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

               
                <Button
                    android:id="@+id/imgb_aceptarNom"
                    style="@style/Button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/str_revisionitem_btn_guardarRevision" />

            </LinearLayout>
            </LinearLayout>
            </ScrollView>
            </FrameLayout>
            
        